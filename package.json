  "devDependencies": {
    "@types/ejs": "3.1.5",
    "@types/express": "5.0.0",
    "@types/node": "22.10.7",
    "nodemon": "3.1.9",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  }
}
